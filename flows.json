    {
        "id": "4315e8e8f0020648",
        "type": "ui_template",
        "z": "33c9ba664b8cc720",
        "group": "78d5b0eeb6db1f0d",
        "name": "Custom UI",
        "order": 1,
        "width": 0,
        "height": 0,
        "format": "<div style=\"text-align:center;\">\n    <h2>Humidity Status</h2>\n    <div id=\"status-box\">Status: —</div>\n    <div id=\"value-box\">— %</div>\n</div>\n\n<style>\n    h2 {\n        color: #00aaff;\n        font-family: Verdana;\n    }\n\n    #status-box,\n    #value-box {\n        font-size: 20px;\n        margin-top: 10px;\n    }\n</style>\n\n<script>\n    (function(scope) {\n    scope.$watch('msg', function(msg) {\n        if (!msg) return;\n\n        document.getElementById(\"value-box\").innerText = msg.payload + \" %\";\n\n        let status = (msg.payload < 40) \n            ? \"Dry\" \n            : (msg.payload < 70) \n                ? \"Comfortable\" \n                : \"Humid\";\n\n        document.getElementById(\"status-box\").innerText = \"Status: \" + status;\n    });\n})(scope);\n</script>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
        "x": 490,
        "y": 440,
